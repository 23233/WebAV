{
  "name": "@webav/av-cliper",
  "version": "0.2.6",
  "private": false,
  "repository": "https://github.com/hughfenghen/WebAV",
  "keywords": [
    "webcodecs",
    "canvas",
    "video",
    "video edit",
    "audio",
    "音视频",
    "cliper",
    "流媒体",
    "recorder"
  ],
  "type": "module",
  "types": "src/index.ts",
  "module": "dist/av-cliper.js",
  "main": "dist/av-cliper.umd.cjs",
  "scripts": {
    "dev": "vite demo --host 0.0.0.0 --port 6066",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "build": "vite build",
    "build:dev": "concurrently --kill-others \"vite build --watch\"",
    "build:demo": "vite build -c demo.vite.config.ts && cp -r demo/public demo-dist/demo",
    "prepublishOnly": "yarn build",
    "preview": "vite preview"
  },
  "files": [
    "dist",
    "src"
  ],
  "devDependencies": {
    "@types/dom-webcodecs": "^0.1.7",
    "@vitest/coverage-istanbul": "^0.29.8",
    "concurrently": "^8.2.0",
    "jsdom": "^21.1.1",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vitest": "^0.29.7"
  },
  "dependencies": {
    "@webav/mp4box.js": "0.5.3-fenghen",
    "wave-resampler": "^1.0.0"
  }
}
